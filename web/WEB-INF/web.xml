<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>UploadData</servlet-name>
        <servlet-class>UploadData</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UploadData</servlet-name>
        <url-pattern>/UploadData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DownloadData</servlet-name>
        <servlet-class>DownloadData</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DownloadData</servlet-name>
        <url-pattern>/DownloadData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Registered</servlet-name>
        <servlet-class>Registered</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Registered</servlet-name>
        <url-pattern>/Registered</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
</web-app>
